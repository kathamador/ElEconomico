<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    tools:context=".ActivityListadoCarnes">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layoutHeader"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/header_backgroud"
        android:padding="9dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-16dp">

        <ImageButton
            android:id="@+id/btn_regresar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="45dp"
            android:background="@color/colorPrimary"
            android:focusable="true"
            android:scaleType="fitCenter"
            android:src="@drawable/leftarrow"
            android:textAllCaps="false"
            android:textColor="@color/colorWhite"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.015"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.26" />


        <TextView
            android:id="@+id/textCarnes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/quicksand_bold"
            android:text="Carnes"
            android:textColor="@color/colorWhite"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:overScrollMode="never"
        android:scrollbars="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layoutHeader">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/layoutChuleta"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="10dp"
                app:layout_constraintBottom_toTopOf="@+id/layoutPollo"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/chuleta"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_rectangle_background"
                    android:padding="5dp">

                    <ImageView
                        android:id="@+id/imageChuleta"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_margin="5dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:contentDescription="@string/app_name"
                        android:padding="5dp"
                        android:src="@drawable/chuleta"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="1.0" />

                    <TextView
                        android:id="@+id/textChuleta"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:gravity="center"
                        android:text="@string/chuleta_de_cerdo_x_lb"
                        android:fontFamily="@font/quicksand_bold"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toTopOf="@+id/textPrecioC"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/imageChuleta"
                        app:layout_constraintTop_toTopOf="@+id/imageChuleta" />

                    <TextView
                        android:id="@+id/textPrecioC"
                        android:layout_width="97dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/quicksand_bold"
                        android:text="L.38.00"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toEndOf="@+id/imageChuleta"
                        app:layout_constraintTop_toBottomOf="@+id/textChuleta" />

                    <LinearLayout
                        android:id="@+id/QuantityC"
                        android:layout_width="90dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="23dp"
                        android:background="@drawable/back"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/imageChuleta"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioC">

                        <FrameLayout
                            android:layout_width="5dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/minus1"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_minus" />
                        </RelativeLayout>

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <TextView
                            android:id="@+id/number1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/circle_back"
                            android:gravity="center"
                            android:text="0"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="12dp" />

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/plus1"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_plus" />
                        </RelativeLayout>


                    </LinearLayout>


                    <Button
                        android:id="@+id/btnagregarChuleta"
                        style="@style/Widget.AppCompat.Button"
                        android:layout_width="100dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="7dp"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/boton"
                        android:drawableStart="@drawable/agregar"
                        android:fontFamily="@font/asap_semibold"
                        android:paddingStart="5dp"
                        android:shadowColor="#070606"
                        android:text="Agregar al Carrito"
                        android:textSize="11sp"
                        app:layout_constraintBottom_toBottomOf="@+id/QuantityC"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/QuantityC"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioC">

                    </Button>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutPollo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10dp"
                app:layout_constraintBottom_toTopOf="@+id/layoutCostilla"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layoutChuleta">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/Pollo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_rectangle_background"
                    android:gravity="center"
                    android:padding="5dp"
                    app:layout_constraintDimensionRatio="H,1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imagePollo"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_margin="5dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:contentDescription="@string/app_name"
                        android:padding="5dp"
                        android:src="@drawable/piernamuslo"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="1.0" />

                    <TextView
                        android:id="@+id/textPollo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:gravity="center"
                        android:text="@string/pierna_con_muslo_x_lb"
                        android:fontFamily="@font/quicksand_bold"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toTopOf="@+id/textPrecioP"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/imagePollo"
                        app:layout_constraintTop_toTopOf="@+id/imagePollo" />

                    <TextView
                        android:id="@+id/textPrecioP"
                        android:layout_width="97dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:text="L.40.00"
                        android:fontFamily="@font/quicksand_bold"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toEndOf="@+id/imagePollo"
                        app:layout_constraintTop_toBottomOf="@+id/textPollo" />

                    <LinearLayout
                        android:id="@+id/QuantityP"
                        android:layout_width="90dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="23dp"
                        android:background="@drawable/back"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/imagePollo"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioP">

                        <FrameLayout
                            android:layout_width="5dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/minus2"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_minus" />
                        </RelativeLayout>

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <TextView
                            android:id="@+id/number2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/circle_back"
                            android:gravity="center"
                            android:text="0"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="12dp" />

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/plus2"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_plus" />
                        </RelativeLayout>


                    </LinearLayout>


                    <Button
                        android:id="@+id/btnagregarPollo"
                        style="@style/Widget.AppCompat.Button"
                        android:layout_width="100dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="7dp"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/boton"
                        android:drawableStart="@drawable/agregar"
                        android:fontFamily="@font/asap_semibold"
                        android:paddingStart="5dp"
                        android:shadowColor="#070606"
                        android:text="Agregar al Carrito"
                        android:textSize="11sp"
                        app:layout_constraintBottom_toBottomOf="@+id/QuantityP"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/QuantityP"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioP">

                    </Button>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutCostilla"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10dp"
                app:layout_constraintBottom_toTopOf="@+id/layoutLomo"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layoutPollo">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/Costilla"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_rectangle_background"
                    android:gravity="center"
                    android:padding="5dp"
                    app:layout_constraintDimensionRatio="H,1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imageCostilla"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_margin="5dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:contentDescription="@string/app_name"
                        android:padding="5dp"
                        android:src="@drawable/costilla"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="1.0" />

                    <TextView
                        android:id="@+id/textCostilla"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:gravity="center"
                        android:text="@string/costilla_de_cerdo_x_lb"
                        android:fontFamily="@font/quicksand_bold"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toTopOf="@+id/textPrecioCo"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/imageCostilla"
                        app:layout_constraintTop_toTopOf="@+id/imageCostilla" />

                    <TextView
                        android:id="@+id/textPrecioCo"
                        android:layout_width="97dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:text="@string/l_30_00"
                        android:fontFamily="@font/quicksand_bold"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toEndOf="@+id/imageCostilla"
                        app:layout_constraintTop_toBottomOf="@+id/textCostilla" />

                    <LinearLayout
                        android:id="@+id/QuantityCo"
                        android:layout_width="90dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="23dp"
                        android:background="@drawable/back"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/imageCostilla"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioCo">

                        <FrameLayout
                            android:layout_width="5dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/minus3"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_minus" />
                        </RelativeLayout>

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <TextView
                            android:id="@+id/number3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/circle_back"
                            android:gravity="center"
                            android:text="0"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="12dp" />

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/plus3"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_plus" />
                        </RelativeLayout>


                    </LinearLayout>


                    <Button
                        android:id="@+id/btnagregarCostilla"
                        style="@style/Widget.AppCompat.Button"
                        android:layout_width="100dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="7dp"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/boton"
                        android:drawableStart="@drawable/agregar"
                        android:fontFamily="@font/asap_semibold"
                        android:paddingStart="5dp"
                        android:shadowColor="#070606"
                        android:text="Agregar al Carrito"
                        android:textSize="11sp"
                        app:layout_constraintBottom_toBottomOf="@+id/QuantityCo"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/QuantityCo"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioCo">

                    </Button>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutLomo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layoutCostilla">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/lomo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/white_rectangle_background"
                    android:gravity="center"
                    android:padding="5dp"
                    app:layout_constraintDimensionRatio="H,1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imagelomo"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_margin="5dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="8dp"
                        android:contentDescription="@string/app_name"
                        android:padding="5dp"
                        android:src="@drawable/lomo"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="1.0" />

                    <TextView
                        android:id="@+id/textlomo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:gravity="center"
                        android:text="@string/lomo_de_cerdo_x_lb"
                        android:fontFamily="@font/quicksand_bold"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toTopOf="@+id/textPrecioL"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toEndOf="@+id/imagelomo"
                        app:layout_constraintTop_toTopOf="@+id/imagelomo" />

                    <TextView
                        android:id="@+id/textPrecioL"
                        android:layout_width="97dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:text="@string/l_38_00"
                        android:fontFamily="@font/quicksand_bold"
                        android:textColor="@color/colorAccent"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toEndOf="@+id/imagelomo"
                        app:layout_constraintTop_toBottomOf="@+id/textlomo" />

                    <LinearLayout
                        android:id="@+id/QuantityL"
                        android:layout_width="90dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="23dp"
                        android:background="@drawable/back"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/imagelomo"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioL">

                        <FrameLayout
                            android:layout_width="5dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/minus4"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_minus" />
                        </RelativeLayout>

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <TextView
                            android:id="@+id/number4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/circle_back"
                            android:gravity="center"
                            android:text="0"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:textSize="12dp" />

                        <FrameLayout
                            android:layout_width="2dp"
                            android:layout_height="match_parent" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <Button
                                    android:id="@+id/plus4"
                                    style="?android:attr/borderlessButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_centerVertical="true"
                                    android:background="@drawable/back_btns"></Button>
                            </RelativeLayout>

                            <ImageView
                                android:layout_width="13dp"
                                android:layout_height="13dp"
                                android:layout_centerInParent="true"
                                android:src="@drawable/ic_plus" />
                        </RelativeLayout>


                    </LinearLayout>


                    <Button
                        android:id="@+id/btnagregarLomo"
                        style="@style/Widget.AppCompat.Button"
                        android:layout_width="100dp"
                        android:layout_height="35dp"
                        android:layout_marginStart="7dp"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/boton"
                        android:drawableStart="@drawable/agregar"
                        android:fontFamily="@font/asap_semibold"
                        android:paddingStart="5dp"
                        android:shadowColor="#070606"
                        android:text="Agregar al Carrito"
                        android:textSize="11sp"
                        app:layout_constraintBottom_toBottomOf="@+id/QuantityL"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/QuantityL"
                        app:layout_constraintTop_toBottomOf="@+id/textPrecioL">

                    </Button>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>